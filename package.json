{
  "name": "singulix-server",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "prisma:push": "prisma db push",
    "prisma:studio": "prisma studio",
    "seed": "tsx src/seed.ts",
    "lint": "eslint .",
    "postinstall": "prisma generate",
    "db:deploy": "prisma migrate deploy",
    "db:dev": "prisma migrate dev --name init",
    "db:push": "prisma db push",
    "user:create": "node --env-file=.env scripts/createUser.js",
    "migrate:init": "prisma migrate dev --name init",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:reset": "prisma migrate reset"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "argon2": "^0.40.1",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "csv-stringify": "^6.5.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "zod": "^3.23.8",
    "@vercel/blob": "^0.27.0"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.19.17",
    "eslint": "^9.9.0",
    "prisma": "^5.22.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4"
  }
}